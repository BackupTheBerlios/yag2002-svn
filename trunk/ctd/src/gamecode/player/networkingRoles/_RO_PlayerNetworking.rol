/* The chat member object */
object PlayerNetworking
{
    
    datablock Function;
    extern RPC_AddChatText(tChatMsg);
    
    datablock Predict_Float;
    
    datablock NData;
    
    networking
    {

        NData _p_playerName
        { 
            Reliable();
        }
        NData _p_configFile
        { 
            Reliable();
        }

        Predict_Float _positionX;
        {
            SetMinError(0.1f);
            SetMinDistance(6.0f);
            SetMaxError(0.3f);
            SetMaxDistance(10.0f);
            Quadratic();
        }
        Predict_Float _positionY;
        {
            SetMinError(0.1f);
            SetMinDistance(6.0f);
            SetMaxError(0.3f);
            SetMaxDistance(10.0f);
            Quadratic();
        }
        Predict_Float _positionZ;
        {
            SetMinError(0.1f);
            SetMinDistance(6.0f);
            SetMaxError(0.3f);
            SetMaxDistance(10.0f);
            Quadratic();
        }

        Predict_Float _yaw;
        {
            SetMinError(0.1f);
            SetMaxError(0.2f);
            Linear();
        }

        NData _cmdAnimFlags;
        { 
            Reliable();
        }

        Function RPC_AddChatText;
        { 
            Reliable();
        }
    }
     
}
