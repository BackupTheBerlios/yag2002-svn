CREATE FUNCTION user_getaccountdata(login_name character varying) RETURNS user_accounts
    AS $$
DECLARE

   user_account    user_accounts%ROWTYPE;

   -- online flag
   fonline         INTEGER := 1;

BEGIN

    SELECT INTO user_account * FROM user_accounts WHERE user_accounts.login_name = login_name;

    -- check if the user exists
    IF ( user_account IS NULL ) THEN
        RETURN NULL;
    END IF;

    -- reset the online flag
    IF ( user_account.status & fonline ) THEN
        -- clear the passwd field
        user_account.passwd = '';
        RETURN user_account;
    END IF;

    RETURN NULL;

END;
$$
    LANGUAGE plpgsql;


