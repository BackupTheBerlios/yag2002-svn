CREATE FUNCTION user_register(r_login_name character varying, r_passwd character varying, r_name character varying, r_email character varying) RETURNS integer
    AS $$
DECLARE

   cnt             integer := 0;
   acc_id          integer := 0;
   data_id         integer := 0;

BEGIN

    SELECT INTO cnt count(*) FROM user_accounts WHERE user_accounts.login_name = r_login_name;

    -- check if a user with given login name already exists
    IF ( cnt > 0 ) THEN
        RETURN -1;
    END IF;

    acc_id := nextval('user_accounts_account_id_seq'); 
    -- create user account, set also the last login column
    INSERT INTO user_accounts ( account_id, login_name, passwd, name, email, last_login ) VALUES ( acc_id, r_login_name, r_passwd, r_name, r_email, now() );

    -- create the user data and set the reference to account
    data_id := nextval('user_data_user_data_id_seq'); 
    INSERT INTO user_data ( user_data_id, user_account_id ) VALUES ( data_id, acc_id );

    --create inventory
    INSERT INTO inventory ( user_data_id ) VALUES ( data_id );

    -- create skill
    INSERT INTO skills ( user_data_id ) VALUES ( data_id );

    -- create mailbox
    INSERT INTO mailboxes ( user_data_id ) VALUES ( data_id );

    RETURN 1;

END;
$$
    LANGUAGE plpgsql;


